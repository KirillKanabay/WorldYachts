<UserControl x:Class="WorldYachts.View.DashboardView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WorldYachts.View"
             xmlns:vm="clr-namespace:WorldYachts.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" FontFamily="SF Pro Display" FontSize="16">
    <UserControl.DataContext>
        <vm:DashboardViewModel></vm:DashboardViewModel>
    </UserControl.DataContext>
    <Grid Background="#FFF6F5F8">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Horizontal">
            <TextBlock>Dashboard</TextBlock>
            <TextBlock Text="{Binding  Name}" Margin="5 0 "></TextBlock>
            <TextBlock Text="{Binding SecondName}"></TextBlock>
        </StackPanel>
        
    </Grid>
</UserControl>
