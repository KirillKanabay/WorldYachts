<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:WorldYachts.ViewModel"
                    xmlns:v="clr-namespace:WorldYachts.View"
                    xmlns:vmcm="clr-namespace:WorldYachts.ViewModel.CatalogManagementViewModels"
                    xmlns:vcm="clr-namespace:WorldYachts.View.CatalogManagementViews">
    <ResourceDictionary.MergedDictionaries>
        <!--Основные UC-->
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type vm:DashboardViewModel}">
                <v:DashboardView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:CatalogViewModel}">
                <v:CatalogView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:OrdersViewModel}">
                <v:OrdersView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:UserManagementViewModel}">
                <v:UserManagementView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:AccountSettingsViewModel}">
                <v:AccountSettingsView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:AboutProgramViewModel}">
                <v:AboutProgramView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:CatalogManagementViewModel}">
                <v:CatalogManagementView/>
            </DataTemplate>
        </ResourceDictionary>
        <!--CatalogManagement-->
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type vmcm:AddBoatViewModel}">
                <vcm:AddBoatView/>
            </DataTemplate>
        </ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>